<h1 class="create">Create Post</h1>
<br>
<br>

  <form action="/compose" method="POST">
    <div class="form-group">
      <label for="postTile">Title</label>
      <input type="text" class="form-control" name="postTitle" placeholder="Enter  Title" autofocus required>
    </div>

    <div class="form-group">
      <label for="postContent">Post</label>
      <textarea class="form-control" name="postContent" rows="3" autofocus required></textarea>
    </div>
<br>
    <button type="submit" class="btn btn-primary">Publish</button>
  </form>















